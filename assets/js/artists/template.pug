el-row
  el-col(v-for='(artist, index) in artists', :key='index', :span='6')
    el-card(:body-style="{ padding: '0px' }")
      img.image(:src='artist.imageUrl')
      div(style='padding: 14px;')
        span {{ artist.title }}
        .bottom.clearfix
          span.origin {{ artist.origin }}
        .bottom.clearfix
          ul.channels
            channel(v-for='channel in artist.channels', :key='channel.channelId', :type='channel.type', :channelId='channel.channelId')
