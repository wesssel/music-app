el-row
  el-col(v-for='(artist, index) in artists' v-bind:key='index' v-bind:span='6')
    el-card(:body-style="{ padding: '0px' }")
      img.image(v-bind:src='artist.imageUrl')
      div(style='padding: 14px;')
        span {{ artist.title }}
        .bottom.clearfix
          span.origin {{ artist.origin }}
        .bottom.clearfix
          ul.channels
            channel(v-for='channel in artist.channels' v-bind:key='channel.channelId' v-bind:type='channel.type' v-bind:channelId='channel.channelId')
