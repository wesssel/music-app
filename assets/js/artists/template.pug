el-container
  el-header
    h1 Artists
  el-main
    el-row
      el-col(v-for='(artist, index) in artists', v-bind:span='6')
        el-card(:body-style="{ padding: '0px' }")
          img.image(v-bind:src='artist.imageUrl')
          div(style='padding: 14px;')
            span {{ artist.title }}
            .bottom.clearfix
              span.origin {{ artist.origin }}
